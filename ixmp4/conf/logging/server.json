{
  "version": 1,
  "disable_existing_loggers": true,
  "formatters": {
    "generic": {
      "format": "[%(levelname)s] %(asctime)s - %(name)s: %(message)s",
      "datefmt": "%H:%M:%S"
    }
  },
  "loggers": {
    "ixmp4": {
      "level": "NOTSET",
      "formatter": "generic"
    },
    "litestar": {
      "level": "NOTSET"
    },
    "httpx": {
      "level": "NOTSET"
    },
    "sqlalchemy": {
      "level": "NOTSET"
    },
    "uvicorn.access": {
      "level": "NOTSET"
    },
    "uvicorn": {
      "level": "NOTSET"
    },
    "watchfiles.main": {
      "level": "ERROR"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "generic"
    }
  },
  "root": {
    "level": "DEBUG",
    "handlers": ["console"]
  }
}
