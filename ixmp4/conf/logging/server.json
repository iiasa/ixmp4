{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters":{
    "generic": {
      "format": "[%(levelname)s] %(asctime)s - %(name)s: %(message)s",
      "datefmt": "%H:%M:%S"
    }
  },
  "loggers": {
    "fastapi": {
      "level": "NOTSET",
      "handlers": ["console"]
    },
    "httpx": {
      "level": "NOTSET",
      "handlers": ["console"]
    },
    "sqlalchemy": {
      "level": "NOTSET",
      "handlers": ["console"]
    },
    "uvicorn.access": {
      "level": "NOTSET",
      "handlers": ["console"]
    },
    "watchfiles.main": {
      "level": "ERROR",
      "handlers": ["console"]
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "generic"
    }
  },
  "root": { 
    "level": "DEBUG",
    "handlers": ["console"]
  }
}