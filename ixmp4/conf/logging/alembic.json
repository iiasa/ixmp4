{
  "version": 1,
  "disable_existing_loggers": false,
  "formatters": {
    "generic": {
      "format": "[%(levelname)s] %(asctime)s - %(name)s: %(message)s",
      "datefmt": "%H:%M:%S"
    }
  },
  "root": {
    "level": "WARNING",
    "handlers": ["console"]
  },
  "loggers": {
    "alembic": {
      "level": "INFO"
    },
    "sqlalchemy": {
      "level": "WARNING"
    }
  },
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "NOTSET",
      "formatter": "generic"
    }
  }
}
